<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/LoginR"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:fitsSystemWindows="true"
    android:background="#F0F0F0" >

    <RelativeLayout
        android:id="@+id/R1"
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:background="#179efc" >
        <ImageView
            android:id="@+id/login_reback_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:padding="10dp"
            android:onClick="bt_back"
            android:src="@mipmap/left_arrow_icon"
            />
        <Button
            android:id="@+id/back"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:background="@null"
            android:textColor="#fff"
            android:textSize="15sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="考试结果详情"
            android:textColor="@color/white"
            android:textSize="20sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/R2"
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:layout_below="@+id/R1"
        android:background="@color/white" >

        <TextView
            android:id="@+id/te1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:text="在线考试"
            android:textColor="@color/black"
            android:textSize="17sp" />

        <TextView
            android:id="@+id/te2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:paddingLeft="6dp"
            android:text="0/0"
            android:textColor="@color/galy1"
            android:textSize="16sp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/te2"
            android:src="@mipmap/location" />
    </RelativeLayout>

    <TextView
        android:id="@+id/line"
        android:layout_width="fill_parent"
        android:layout_height="0.2dp"
        android:layout_below="@+id/R2"
        android:background="@color/galy1" />

    <LinearLayout
        android:id="@+id/check_10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/line"
        android:layout_gravity="center"
        android:layout_marginTop="8dp"
        android:orientation="horizontal"
        android:paddingLeft="18dp" >

        <ImageView
            android:id="@+id/login_check10"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_gravity="center"
            android:src="@drawable/checkbox_c"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/login_checkt10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="8dp"
            android:text="只看错题"
            android:textColor="@color/black"
            android:textSize="16sp" />
    </LinearLayout>

    <ScrollView android:id="@+id/LL1"
        android:layout_width="fill_parent"
        android:layout_below="@+id/check_10"
        android:layout_above="@+id/bt1"
        android:layout_height="wrap_content" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:orientation="vertical"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/show"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_margin="16dp"
                android:text=""
                android:lineSpacingExtra="4dp"
                android:textColor="@color/black"
                android:textSize="18sp" />

            <LinearLayout
                android:id="@+id/check_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="13dp"
                android:orientation="horizontal"
                android:paddingLeft="18dp" >

                <ImageView
                    android:id="@+id/login_check1"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_gravity="center"
                    android:textColor="@color/black" />

                <TextView
                    android:id="@+id/login_checkt1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="8dp"
                    android:text=""
                    android:textColor="@color/black"
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/check_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/check_1"
                android:layout_marginTop="13dp"
                android:orientation="horizontal"
                android:paddingLeft="18dp" >

                <ImageView
                    android:id="@+id/login_check2"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_gravity="center"
                    android:textColor="@color/black" />

                <TextView
                    android:id="@+id/login_checkt2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="8dp"
                    android:text=""
                    android:textColor="@color/black"
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/check_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/check_2"
                android:layout_marginTop="13dp"
                android:orientation="horizontal"
                android:paddingLeft="18dp" >

                <ImageView
                    android:id="@+id/login_check3"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_gravity="center"
                    android:textColor="@color/black" />

                <TextView
                    android:id="@+id/login_checkt3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="8dp"
                    android:text=""
                    android:textColor="@color/black"
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/check_4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/check_3"
                android:layout_marginTop="13dp"
                android:orientation="horizontal"
                android:paddingLeft="18dp" >

                <ImageView
                    android:id="@+id/login_check4"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_gravity="center"
                    android:textColor="@color/black" />

                <TextView
                    android:id="@+id/login_checkt4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="8dp"
                    android:text=""
                    android:textColor="@color/black"
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/check_5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/check_4"
                android:layout_marginTop="13dp"
                android:orientation="horizontal"
                android:paddingLeft="18dp" >

                <ImageView
                    android:id="@+id/login_check5"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_gravity="center"
                    android:textColor="@color/black" />

                <TextView
                    android:id="@+id/login_checkt5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="8dp"
                    android:text=""
                    android:textColor="@color/black"
                    android:textSize="18sp" />
            </LinearLayout>
            
            <TextView
		        android:id="@+id/zong"
		        android:layout_width="fill_parent"
		        android:layout_marginTop="12dp"
		        android:gravity="center"
		        android:layout_height="wrap_content"
		        android:text=""
		        android:textSize="17sp"
		        android:textColor="@color/red" />
        </LinearLayout>
    </ScrollView>

    <TextView
        android:id="@+id/text1"
        android:layout_width="1dp"
        android:layout_height="2dp"
        android:layout_centerInParent="true" />

    <Button
        android:id="@+id/bt1"
        android:layout_width="120dip"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="40dp"
        android:layout_marginRight="25dip"
        android:layout_toLeftOf="@+id/text1"
        android:background="@drawable/out1_selector4"
        android:textColor="@color/white"
        android:padding="11dip"
        android:text="上一题" />

    <Button
        android:id="@+id/bt2"
        android:layout_width="120dip"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="40dp"
        android:layout_marginLeft="25dip"
        android:layout_toRightOf="@+id/text1"
        android:background="@drawable/out1_selector4"
        android:padding="11dip"
        android:textColor="@color/white"
        android:text="下一题" />

</RelativeLayout>